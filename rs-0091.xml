name="bibtex"><![CDATA[@book{maclane-moerdijk-1994,
  title = {Sheaves in Geometry and Logic: A First Introduction to Topos Theory},
  author = {MacLane, S. and Moerdijk, I.},
  isbn = {9781461209270},
